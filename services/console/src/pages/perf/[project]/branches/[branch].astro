---
export const prerender = false;

import PerfResource from "../../../../layouts/perf/PerfResource.astro";
import branchesPubConfig from "../../../../config/project/branches_pub";
import { PubResource } from "../../../../util/resource";
---

<PerfResource
  title={(json) => json ? `${json?.name}` : "Public Branch"}
  description={(json) => json ? `View a continuous benchmarking branch ${json?.name} Bencher` : "View a continuous benchmarking branch"}
  created={(json) => json ? json.created : ""}
  modified={(json) => json ? json.modified : ""}
  url={(params) => `/v0/projects/${params.project}/branches/${params.branch}`}
  resource={PubResource.Branch}
  config={branchesPubConfig}
/>
