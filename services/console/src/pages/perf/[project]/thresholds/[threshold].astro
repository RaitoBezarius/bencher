---
export const prerender = false;

import PerfResource from "../../../../layouts/perf/PerfResource.astro";
import thresholdsPubConfig from "../../../../config/project/thresholds_pub";
---

<PerfResource
  title={(json) => json ? `${json?.branch?.name} | ${json?.testbed?.name} | ${json?.measure?.name}` : "Public Threshold"}
  description={(_json) => "View a continuous benchmarking threshold on Bencher"}
  created={(json) => json?.created ?? ""}
  modified={(json) => json?.modified ?? ""}
  config={thresholdsPubConfig}
/>
