{
  "git.autofetch": true,
  "files.eol": "\n",
  "files.associations": {
    "*.mdx": "markdown"
  },
  "editor.formatOnSave": true,
  // https://rust-analyzer.github.io/manual.html
  "rust-analyzer.imports.granularity.group": "crate",
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.linkedProjects": [
    "Cargo.toml",
  ],
  // "rust-analyzer.check.allTargets": false,
  "biome.lspBin": "./services/console/node_modules/.bin/biome",
  // https://code.visualstudio.com/docs/languages/identifiers#_known-language-identifiers
  "[rust]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[astro]": {
    "editor.formatOnSave": true,
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "biomejs.biome",
    "editor.codeActionsOnSave": {
      "source.organizeImports.biome": "explicit"
    },
    // "editor.codeActionsOnSave": {
    //   "source.organizeImports.biome": true
    // }
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "biomejs.biome",
    "editor.codeActionsOnSave": {
      "source.organizeImports.biome": "explicit"
    },
    // "editor.codeActionsOnSave": {
    //   "source.organizeImports.biome": true
    // }
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "biomejs.biome",
    "editor.codeActionsOnSave": {
      "source.organizeImports.biome": "explicit"
    },
    // "editor.codeActionsOnSave": {
    //   "source.organizeImports.biome": true
    // }
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "bencher",
      "database": "${workspaceFolder:bencher}/services/api/data/bencher.db"
    }
  ],
  "sqltools.autoOpenSessionFiles": false,
  "sqltools.useNodeRuntime": true,
  "cSpell.words": [
    "anychar",
    "Astro",
    "AUTOINCREMENT",
    "bencherdev",
    "bindgen",
    "Bulma",
    "callgrind",
    "camino",
    "canonicalize",
    "chrono",
    "codegen",
    "devcontainer",
    "devel",
    "Dogfooding",
    "dropshot",
    "fullwidth",
    "Insertable",
    "interquartile",
    "jsonwebtoken",
    "libtest",
    "litestream",
    "luhn",
    "mdx",
    "Microbenchmark",
    "milli",
    "minijinja",
    "nanos",
    "octocrab",
    "openapi",
    "Perfs",
    "pipenv",
    "pytest",
    "repr",
    "reqwest",
    "Roadmap",
    "rqctx",
    "rustls",
    "schemars",
    "signup",
    "starttls",
    "thiserror",
    "typeshare",
    "UNIONPAY",
    "valgrind",
    "vcentered",
    "wordmark"
  ]
}